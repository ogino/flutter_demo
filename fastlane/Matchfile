git_url(ENV["GIT_URL"])

storage_mode("git")

type(ENV["prv_type"]) # The default type, can be: appstore, adhoc, enterprise or development

app_identifier([ENV["APP_IDENTIFIER"]])
username(ENV["APPLE_ID"]) # Your Apple Developer Portal username

team_id(ENV["TEAM_ID"])
team_name(ENV["TEAM_NAME"])

